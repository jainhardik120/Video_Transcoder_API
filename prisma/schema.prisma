generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


enum videoStatus {
  INITIAL
  UPLOADING_RAW
  QUEUED
  CONVERTING
  ERROR
  UPLOADING_CONVERTED
  FAILED
}

model video {
  id          String      @id @default(uuid())
  title       String
  status      videoStatus @default(INITIAL)
  rawFileName String      @map("raw_file_name")
}
